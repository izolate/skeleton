{
  "name": "skeleton",
  "version": "2.0.0",
  "description": "Skeleton for a modern JavaScript webapp",
  "main": "app/index.js",
  "scripts": {
    "develop": "npm run js:dev",
    "js:dev": "browserify app/static/js/index.js -t babelify --debug -o app/static/dist/index.js",
    "js:prd": "browserify app/static/js/index.js -t babelify -t uglifyify -o app/static/dist/index.js",
    "start": "node --use_strict server/index",
    "start:dev": "nodemon --use-strict server/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/izolate/skeleton.git"
  },
  "author": "izolate <yoshtalwar@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/izolate/skeleton/issues"
  },
  "homepage": "https://github.com/izolate/skeleton",
  "dependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "body-parser": "^1.13.3",
    "browserify": "^13.0.0",
    "cookie-parser": "^1.3.5",
    "dotenv": "^1.2.0",
    "express": "^4.13.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-csso": "^1.0.0",
    "gulp-if": "^1.2.5",
    "gulp-load-plugins": "^0.10.0",
    "gulp-postcss": "^6.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-stylus": "^2.0.5",
    "gulp-uglify": "^1.2.0",
    "postcss": "^5.0.19",
    "postcss-cssnext": "^2.5.2",
    "postcss-modules": "^0.4.1",
    "pug": "^2.0.0-beta6",
    "require-dir": "^0.3.0",
    "sugarss": "^0.1.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  }
}
